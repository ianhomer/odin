    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0",
    "rest": "^1.3.1",
    "vexflow": "^1.2.84",
    "webpack": "^4.4.1"
  },
  "scripts": {
    "watch": "webpack --watch -d",
    "test-debug": "node_modules/.bin/jest --inspect-brk --runInBand",
