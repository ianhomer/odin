{
  "name": "odin-frontend",
  "version": "0.1.0",
  "description": "Odin Web Application",
  "repository": {
    "type": "git",
    "url": "git@github.com:ianhomer/odin.git"
  },
  "keywords": [
    "rest",
    "hateoas",
